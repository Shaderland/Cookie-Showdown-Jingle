{
	"animations": "animation/scene.json",
	"fonts": {
		"coffee": {
			"file": "font/coffee.json"
		}
	},
	"textures": {
		"spritesheet": {
			"file": "image/spritesheet.png"
		},
		"jonathan": {
			"file": "image/jonathan.jpg"
		},
		"cookie": {
			"file": "image/Cookie.png"
		}
	},
	"geometries": {
		"treeCurves": {
			"file": "mesh/treeCurves.obj"
		},
		"jonathan": {
			"file": "mesh/jonathan.obj"
		},
		"plantPoints": {
			"file": "mesh/plantPoints.ply"
		},
		"cookie": {
			"file": "mesh/Cookie.obj"
		},
		"title": {
			"file": "mesh/title.obj"
		}
	},
	"shaders": {
		"background": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/project/filters/background.frag"
		},
		"raymarching": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/project/raymarch/timelapse.frag"
		},
		"cookie": {
			"vertexShader": "shader/project/meshes/cookie.vert",
			"fragmentShader": "shader/project/meshes/cookie.frag"
		},
		"render": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/project/filters/render.frag"
		},
		"lines": {
			"vertexShader": "shader/project/particles/fire.vert",
			"fragmentShader": "shader/generic/circle.frag",
			"side": 2
		},
		"fire": {
			"vertexShader": "shader/project/particles/fire.vert",
			"fragmentShader": "shader/generic/circle.frag",
			"side": 2
		},
		"firePosition": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/project/datas/firePosition.frag",
			"side": 2
		},
		"fireVelocity": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/project/datas/fireVelocity.frag",
			"side": 2
		},
		"text": {
			"vertexShader": "shader/project/meshes/text.vert",
			"fragmentShader": "shader/project/meshes/text.frag",
			"depthTest": false,
			"opacity": 0.5,
			"transparent": true
		}
	}
}
